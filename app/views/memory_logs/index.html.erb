<h2>Istoric utilizare RAM</h2>
<table class="table">
  <thead>
    <tr>
      <th>Data</th>
      <th>RAM folosit (MB)</th>
      <th>Eliberat (MB)</th>
      <th>Note</th>
    </tr>
  </thead>
  <tbody>
  <% @logs.each do |log| %>
    <tr>
      <td><%= log.created_at.strftime("%d.%m.%Y %H:%M:%S") %></td>
      <td><%= number_with_precision(log.used_memory_mb, precision: 2) %></td>
      <td><%= number_with_precision(log.freed_memory_mb, precision: 2) %></td>
      <td>
        <% if log.note.include?("automat") %>
          <span style="color: green; font-weight: bold;">ðŸŒ€ Automat</span><br>
        <% end %>
        <%= log.note %>
      </td>
    </tr>
  <% end %>
</tbody>

</table>
